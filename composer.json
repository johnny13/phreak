{
    "name": "huement/phreak",
    "description": "Phreak : make your Company/Self/Project stand out",
    "keywords": [
        "framework",
        "laravel",
        "laravel zero",
        "console",
        "cli",
        "design tokens",
        "branding",
        "color palette",
        "design guide",
        "style guide",
        "icons",
        "icon system"
    ],
    "homepage": "https://huement.com/phreak",
    "type": "project",
    "license": "GPL-3.0-or-later",
    "support": {
        "issues": "https://github.com/huement/phreak/issues",
        "source": "https://github.com/huement/phreak"
    },
    "authors": [
        {
            "name": "Derek",
            "email": "derek@huement.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "claviska/simpleimage": "dev-master",
        "codedungeon/php-messenger": "dev-master",
        "dallgoot/yaml": "dev-master",
        "donatj/cli-toolkit": "dev-master",
        "greg0/lazer-database": "dev-master",
        "illuminate/log": "^8.0",
        "imagine/imagine": "^1.2",
        "jc21/clitable": "dev-master",
        "kevinlebrun/colors.php": "dev-master",
        "laminas/laminas-text": "2.10.x-dev",
        "laravel-zero/framework": "^8.8",
        "lastguest/pixeler": "dev-master",
        "league/climate": "dev-master",
        "lukapeharda/tailwindcss-color-palette-generator": "dev-main",
        "meyfa/php-svg": "dev-main",
        "mikealmond/color": "1.0.x-dev",
        "mistic100/randomcolor": "dev-master",
        "nesbot/carbon": "2.x-dev",
        "nstdio/svg": "*",
        "nunomaduro/laravel-console-menu": "^3.2",
        "ourcodeworld/name-that-color": "dev-master",
        "owenmelbz/laravel-stubs": "dev-master",
        "ozdemirburak/iris": "dev-master",
        "projekod/roundcolor": "@dev",
        "ssnepenthe/color-utils": "dev-master",
        "symfony/console": "5.4.x-dev",
        "talesoft/phim": "^1.0",
        "termage/termage": "dev-dev",
        "todstoychev/websafe-colors": "dev-master"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.3",
        "nunomaduro/dig": "dev-master",
        "nunomaduro/phpinsights": "dev-master",
        "pestphp/pest": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "App\\Commands\\Phreaks\\": "phreaks",
            "App\\Commands\\Wallpapers\\": "wallpapers"
        },
        "classmap": [
            "app/Commands/Phreaks/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": false,
    "bin": [
        "phreak"
    ],
    "phpcs.standard": "PSR2"
}
